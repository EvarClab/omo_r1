<launch>
  <arg name="set_model" default="r1"/>
  <arg name="set_multi_robot_name" default=""/>

  <include file="$(find omo_r1_bringup)/launch/includes/description.launch.G2.xml"/>

  <node pkg="robot_state_publisher" type="robot_state_publisher" name="$(arg set_multi_robot_name)robot_state_publisher">
    <param name="publish_frequency" type="double" value="50.0" />
    <param name="tf_prefix" value="$(arg set_multi_robot_name)"/>
  </node>

  <node pkg="joint_state_publisher" type="joint_state_publisher" name="joint_state_publisher">
    <param name="use_gui" value="false"/>
    <param name="rate" value="50"/>
  </node>
</launch>
